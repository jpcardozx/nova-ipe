{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "paths": {
            "@/*": [
                "./*"
            ],
            "@/src/*": [
                "./src/*"
            ],
            "@/lib/*": [
                "./lib/*"
            ],
            "@/components/*": [
                "./components/*"
            ],
            "@/app/*": [
                "./app/*"
            ],
            "@/public/*": [
                "./public/*"
            ]
        ],
        "@sections/*": [
            "./app/sections/*"
        ],
        "@lib/*": [
            "./lib/*"
        ],
        "@/types/*": [
            "./src/types/*"
        ],
        "@types/*": [
            "./src/types/*"
        ]
    }
}
}