{
  "name": "nova-ipe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "set FAST_DEV=true && next dev",
    "dev:turbo": "set FAST_DEV=true && set NEXT_DISABLE_SOURCEMAPS=true && next dev --turbo",
    "dev:memory": "set FAST_DEV=true && set NODE_OPTIONS=--max_old_space_size=2048 && next dev",
    "dev:vitals": "set NEXT_PUBLIC_ENABLE_WEB_VITALS=true && next dev",
    "dev:debug": "set NEXT_PUBLIC_ENABLE_WEB_VITALS=true && set NEXT_PUBLIC_VITALS_DEBUG=true && next dev",
    "dev:loading-profile": "set NEXT_PUBLIC_ENABLE_WEB_VITALS=true && set NEXT_PUBLIC_LOADING_PROFILE=true && next dev",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File clean-dev.ps1",
    "dev:start": "powershell -ExecutionPolicy Bypass -File fast-dev.ps1",
    "fix:placeholder": "node scripts/create-simple-placeholder.js",
    "optimize:images": "node scripts/optimize-images.js",
    "check:performance": "node scripts/verify-performance-improvement.js",
    "diagnose": "node scripts/diagnose-slow-render.js",
    "dev:superfast": "powershell -ExecutionPolicy Bypass -File dev-fast.ps1",
    "prebuild": "node scripts/fix-sourcemaps.mjs && node scripts/build-service-worker.mjs",
    "build": "next build",
    "postbuild": "node scripts/build-service-worker.mjs || echo 'Service worker build failed but continuing...'",
    "start": "next start",
    "lint": "next lint",
    "deploy:prod": "vercel --prod",
    "generate-og-images": "node scripts/fallback-og-images.mjs",
    "clean": "rm -rf .next node_modules/.cache",
    "fix:sourcemaps": "node scripts/fix-sourcemaps.mjs",
    "analyze": "ANALYZE=true next build",
    "test:performance": "node scripts/test-performance.mjs",
    "optimize:all": "optimize-all.bat",
    "test:performance:new": "node scripts/performance-test.mjs",
    "test:pwa": "node scripts/test-pwa.js",
    "verify:performance": "node scripts/performance-test.mjs --compare",
    "optimize": "next build && next start",
    "report:performance": "node scripts/performance-test.mjs --report",
    "build:test": "node scripts/build-and-test.js",
    "vercel-build": "node scripts/fix-sourcemaps.mjs && next build",
    "typecheck": "tsc --noEmit"
  },
