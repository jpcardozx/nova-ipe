{
  "name": "nova-ipe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "build:fast": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit --skipLibCheck --incremental",
    "typecheck:watch": "tsc --noEmit --skipLibCheck --incremental --watch",
    "clean": "rimraf .next out .swc node_modules/.cache",
    "clean:all": "rimraf .next out .swc node_modules/.cache .turbo",
    "dev:clean": "pnpm run clean && next dev --turbo",
    "build:clean": "pnpm run clean && next build",
    "start:clean": "pnpm run clean && next start",
    "diagnostic": "node scripts/check-build-health.js",
    "analyze": "ANALYZE=true next build",
    "perf:audit": "lighthouse http://localhost:3000 --output json --output html",
    "perf:bundle": "npx @next/bundle-analyzer .next/static/chunks/*.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@portabletext/react": "^3.0.15",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@sanity/client": "^7.8.2",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.0.2",
    "@sanity/types": "^4.3",
    "@sanity/ui": "^2.16.12",
    "@sanity/vision": "^3.20.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-virtual": "3.13.12",
    "@types/react-window": "1.8.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "critters": "0.0.25",
    "date-fns": "^2.30.0",
    "embla-carousel": "8.6.0",
    "embla-carousel-autoplay": "8.6.0",
    "embla-carousel-react": "8.6.0",
    "framer-motion": "^12.23.12",
    "lru-cache": "11.1.0",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-sanity": "^10.0.10",
    "next-themes": "^0.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized-auto-sizer": "1.0.26",
    "react-window": "1.8.11",
    "sanity": "^4.3.0",
    "sonner": "2.0.7",
    "styled-components": "6.1.19",
    "tailwind-merge": "^2.0.0",
    "web-vitals": "5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  }
}
