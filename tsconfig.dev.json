{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "skipLibCheck": true,
        "strict": false,
        "noErrorTruncation": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "strictFunctionTypes": false,
        "strictBindCallApply": false,
        "strictPropertyInitialization": false,
        "noImplicitThis": false,
        "useUnknownInCatchVariables": false,
        "alwaysStrict": false,
        "checkJs": false,
        "incremental": true,
        "typeRoots": [
            "./node_modules/@types",
            "./types"
        ]
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx",
        ".next/types/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}